  "scripts": {
    "bundle": "cd android && ./gradlew bundleRelease",
    "android": "cd android && ./gradlew clean && cd .. && react-native run-android",
    "android-release": "cd android && ./gradlew clean && cd .. && react-native run-android --variant=release",
    "android-clean": "cd android && ./gradlew clean",
    "android-build": "react-native bundle --platform android --dev false --entry-file index.js --bundle-output android/app/src/main/assets/index.android.bundle --assets-dest android/app/src/main/res",
    "android-package": "rm -rf ./android/app/src/main/res/drawable-* && cd android && ./gradlew assembleRelease",
    "android-package-debug": "rm -rf ./android/app/src/main/res/drawable-* && cd android && ./gradlew assembleDebug",
    "start": "react-native start --port=8081",
    "prettier:write": "npx prettier --write **/*.{js,jsx,ts,tsx,json} && npx prettier --write *.{js,jsx,ts,tsx,json}",
    "lint": "tsc --noEmit && eslint --ext .js,.jsx,.ts,.tsx ./"
  },
